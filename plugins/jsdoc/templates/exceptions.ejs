<%
    var data = obj;
%>
<% if (data.description && data.type && data.type.names) { %>
<dl>
    <dt>
        <div class="param-desc">
        <%- data.description %>
        </div>
    </dt>
    <dd></dd>
    <dt>
        <dl>
            <dt>
                Type
            </dt>
            <dd>
                <%- include('type.ejs', { ...data.type.names, obj: data.type.names }) %>
            </dd>
        </dl>
    </dt>
    <dd></dd>
</dl>
<% } else { %>
    <div class="param-desc">
    <% if (data.description) { %>
        <%- data.description %>
    <% } else if (data.type && data.type.names) { %>
        <%- include('type.ejs', { ...data.type.names, obj: data.type.names }) %>
    <% } %>
    </div>
<% } %>
